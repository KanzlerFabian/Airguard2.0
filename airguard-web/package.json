{
  "name": "airguard-web",
  "version": "1.0.0",
  "description": "Lokales AirGuard Dashboard (Node/Express + Prometheus Backend).",
  "private": true,
  "type": "commonjs",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server": "ts-node server/index.ts",
